<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8" isELIgnored="false"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>登录</title>
</head>
<body>
<div>
	<form id="loginForm"  class="form-class" action="login" method="post" >
		<h2 class="form-heading">大数据任务管理系统登录</h2>
		<div class="form-body">
		<table>
		<tr>
			<td><label for="username">用户名：</label></td>
			<td><input type="text" name="username" id="username" class="input-block-level" value="用户名"/></td>
			<td><span style="color: red;">${usernameError}</span></td>
		</tr>
		<tr>
			<td><br/></td>
		</tr>
		<tr>
			<td><label for="password">密&nbsp;&nbsp;&nbsp;码：</label></td>
			<td><input type="password" name="password" id="password" class="input-block-level" value="密码"/></td>
			<td><span style="color: red;" >${passwordError}</span></td>
		</tr>
		</table>
		</div>
		<span style="color: red;" >${error}</span>
		<div >
		<input type="submit" name="loginSubmit" id="loginSubmit" value="登录" />
		</div>
	</form>
</div>
</body>
</html>