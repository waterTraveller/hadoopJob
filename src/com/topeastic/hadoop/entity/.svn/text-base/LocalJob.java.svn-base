package com.topeastic.hadoop.entity;

public class LocalJob {

	private String id;
	
	private String jobUserId;
	
	private String jobUserName;
	
	private String jobId;
	
	private String jobName;
	
	private String jobJarName;
	
	private String jobClassName;
	
	private String jobInputPath;
	
	private String jobOutputPath;
	
	private String jobState;
	
	private String jobStartTime;
	
	private String jobEndTime;
	
	private String jobHadoopId;
	

	public String getJobHadoopId() {
		return jobHadoopId;
	}

	public void setJobHadoopId(String jobHadoopId) {
		this.jobHadoopId = jobHadoopId;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getJobUserId() {
		return jobUserId;
	}

	public void setJobUserId(String jobUserId) {
		this.jobUserId = jobUserId;
	}

	public String getJobUserName() {
		return jobUserName;
	}

	public void setJobUserName(String jobUserName) {
		this.jobUserName = jobUserName;
	}

	public String getJobId() {
		return jobId;
	}

	public void setJobId(String jobId) {
		this.jobId = jobId;
	}

	public String getJobName() {
		return jobName;
	}

	public void setJobName(String jobName) {
		this.jobName = jobName;
	}

	public String getJobJarName() {
		return jobJarName;
	}

	public void setJobJarName(String jobJarName) {
		this.jobJarName = jobJarName;
	}

	public String getJobClassName() {
		return jobClassName;
	}

	public void setJobClassName(String jobClassName) {
		this.jobClassName = jobClassName;
	}

	public String getJobInputPath() {
		return jobInputPath;
	}

	public void setJobInputPath(String jobInputPath) {
		this.jobInputPath = jobInputPath;
	}

	public String getJobOutputPath() {
		return jobOutputPath;
	}

	public void setJobOutputPath(String jobOutputPath) {
		this.jobOutputPath = jobOutputPath;
	}

	public String getJobState() {
		return jobState;
	}

	public void setJobState(String jobState) {
		this.jobState = jobState;
	}

	public String getJobStartTime() {
		return jobStartTime;
	}

	public void setJobStartTime(String jobStartTime) {
		this.jobStartTime = jobStartTime;
	}

	public String getJobEndTime() {
		return jobEndTime;
	}

	public void setJobEndTime(String jobEndTime) {
		this.jobEndTime = jobEndTime;
	}

	@Override
	public String toString() {
		return "LocalJob [id=" + id + ", jobUserId=" + jobUserId
				+ ", jobUserName=" + jobUserName + ", jobId=" + jobId
				+ ", jobName=" + jobName + ", jobJarName=" + jobJarName
				+ ", jobClassName=" + jobClassName + ", jobInputPath="
				+ jobInputPath + ", jobOutputPath=" + jobOutputPath
				+ ", jobState=" + jobState + ", jobStartTime=" + jobStartTime
				+ ", jobEndTime=" + jobEndTime + ", jobHadoopId=" + jobHadoopId
				+ "]";
	}

	

}
